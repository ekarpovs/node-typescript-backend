{
  "openapi": "3.0.0",
  "info": {
    "version": "v1.0.0",
    "title": "Swagger Project for node-typescript-backend",
    "description": "Swagger with TypeScript"
  },
  "servers": [
    {
      "url": "http://localhost:3007",
      "description": "Local"
    },
    {
      "url": "https://node-typescript-backend.onrender.com",
      "description": "Render"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "tags": [
          "Application"
        ],
        "description": "Home",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/test-auth": {
      "get": {
        "tags": [
          "Application"
        ],
        "description": "Test a route protected by authentication",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/test-acl": {
      "get": {
        "tags": [
          "Application"
        ],
        "description": "Test a route protected by authentication and authorization",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth": {
      "get": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "Test that the auth is live",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/logout": {
      "get": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/register": {
      "post": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "API to send register data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "example": "any"
                  },
                  "email": {
                    "example": "any"
                  },
                  "password": {
                    "example": "any"
                  },
                  "role": {
                    "example": "user/admin"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "API to send login data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "example": "test_user1@gmail.com"
                  },
                  "password": {
                    "example": "any"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/change-password": {
      "post": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "API to send change password data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "example": "any"
                  },
                  "password": {
                    "example": "any"
                  },
                  "passwordNew": {
                    "example": "any"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/reset-password-request": {
      "post": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "API to send reset password request data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "example": "any"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/reset-password": {
      "post": {
        "tags": [
          "Authentication via auth-session"
        ],
        "description": "API to send reset password data",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "user": {
                    "example": "any"
                  },
                  "token": {
                    "example": "any"
                  },
                  "password": {
                    "example": "any"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  }
}